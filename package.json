{
  "name": "kbc-game",
  "version": "1.0.0",
  "description": "MonoRepo + MicroServices",
  "main": "index.js",
  "packageManager": "pnpm@9.1.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "test": "turbo test",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "folder:remove": "rm -rf node_modules pnpm-lock.yaml",
    "docker:prune": "docker system prune -a -f",
    "docker:prunevolumes": "docker volume prune -f",
    "docker:dev": "docker-compose up --build -d",
    "docker:stop": "docker-compose down"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@types/node": "^20.12.12",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.0.1",
    "typescript": "^5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akhileshojha/kbc-game.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akhileshojha/kbc-game/issues"
  },
  "homepage": "https://github.com/akhileshojha/kbc-game#readme"
}
